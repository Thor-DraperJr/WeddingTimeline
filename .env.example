# Environment Variables for Wedding Timeline App
# Copy this file to .env.local and add your actual values
# NEVER commit .env.local to version control

# Google Maps API Key (for location links)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Azure Application Insights (for monitoring)
NEXT_PUBLIC_APPLICATIONINSIGHTS_CONNECTION_STRING=your_app_insights_connection_string

# Optional: Analytics
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id

# Development only - timezone for testing
NEXT_PUBLIC_TIMEZONE=America/Edmonton

# Wedding specific configuration
NEXT_PUBLIC_WEDDING_DATE=2025-07-11
NEXT_PUBLIC_COUPLE_NAMES="Carolina & Thor"
